  <patternset id="excluded.from.module.pickupspawners">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.pickupspawners">
    <patternset refid="excluded.from.module.pickupspawners"/>
  </patternset>
  
  <path id="pickupspawners.module.sourcepath">
    <dirset dir="${module.pickupspawners.basedir}">
      <include name="src/main/java"/>
      <include name="src/main/resources"/>
    </dirset>
  </path>
  
  <path id="pickupspawners.module.test.sourcepath">
    <dirset dir="${module.pickupspawners.basedir}">
      <include name="src/test/java"/>
      <include name="src/test/resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.pickupspawners" depends="compile.module.pickupspawners.production,compile.module.pickupspawners.tests" description="Compile module PickupSpawners"/>
  
  <target name="compile.module.pickupspawners.production" depends="register.custom.compilers" description="Compile module PickupSpawners; production classes">
    <mkdir dir="${pickupspawners.output.dir}"/>
    <javac2 destdir="${pickupspawners.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.pickupspawners}/javac">
      <compilerarg line="${compiler.args.pickupspawners}"/>
      <bootclasspath refid="pickupspawners.module.bootclasspath"/>
      <classpath refid="pickupspawners.module.production.classpath"/>
      <src refid="pickupspawners.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.pickupspawners"/>
    </javac2>
    
    <copy todir="${pickupspawners.output.dir}">
      <fileset dir="${module.pickupspawners.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.pickupspawners.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.pickupspawners.tests" depends="register.custom.compilers,compile.module.pickupspawners.production" description="compile module PickupSpawners; test classes" unless="skip.tests">
    <mkdir dir="${pickupspawners.testoutput.dir}"/>
    <javac2 destdir="${pickupspawners.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.pickupspawners}/javac">
      <compilerarg line="${compiler.args.pickupspawners}"/>
      <bootclasspath refid="pickupspawners.module.bootclasspath"/>
      <classpath refid="pickupspawners.module.classpath"/>
      <src refid="pickupspawners.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.pickupspawners"/>
    </javac2>
    
    <copy todir="${pickupspawners.testoutput.dir}">
      <fileset dir="${module.pickupspawners.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.pickupspawners.basedir}/src/test/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.pickupspawners" description="cleanup module">
    <delete dir="${pickupspawners.output.dir}"/>
    <delete dir="${pickupspawners.testoutput.dir}"/>
  </target>
