  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <fileset dir="${javac2.home}">
      <include name="javac2.jar"/>
      <include name="jdom.jar"/>
      <include name="asm-all*.jar"/>
      <include name="forms-*.jar"/>
    </fileset>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  <import file="${basedir}/module_pickupspawners.xml"/>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.pickupspawners" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.pickupspawners" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.pickupspawners:jar" value="${basedir}/../../../Desktop/szerver/versiontest/plugins/PickupSpawners_jar"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.PickupSpawners.jar" value="${artifacts.temp.dir}/PickupSpawners.jar"/>
  </target>
  
  <target name="artifact.pickupspawners:jar" depends="init.artifacts, compile.module.pickupspawners" description="Build &#39;PickupSpawners:jar&#39; artifact">
    <property name="artifact.temp.output.pickupspawners:jar" value="${artifacts.temp.dir}/PickupSpawners_jar"/>
    <mkdir dir="${artifact.temp.output.pickupspawners:jar}"/>
    <jar destfile="${temp.jar.path.PickupSpawners.jar}" duplicate="preserve" filesetmanifest="mergewithoutmain">
      <zipfileset dir="${pickupspawners.output.dir}"/>
      <zipfileset src="${path.variable.maven_repository}/com/google/code/findbugs/jsr305/1.3.9/jsr305-1.3.9.jar"/>
      <zipfileset src="${path.variable.maven_repository}/commons-io/commons-io/2.4/commons-io-2.4.jar"/>
      <zipfileset src="${path.variable.maven_repository}/org/apache/logging/log4j/log4j-api/2.0-beta9/log4j-api-2.0-beta9.jar"/>
      <zipfileset src="${path.variable.maven_repository}/commons-codec/commons-codec/1.9/commons-codec-1.9.jar"/>
      <zipfileset src="${path.variable.maven_repository}/org/apache/commons/commons-lang3/3.3.2/commons-lang3-3.3.2.jar"/>
      <zipfileset src="${path.variable.maven_repository}/com/mojang/authlib/1.5.21/authlib-1.5.21.jar"/>
      <zipfileset src="${path.variable.maven_repository}/com/google/code/gson/gson/2.8.0/gson-2.8.0.jar"/>
      <zipfileset src="${path.variable.maven_repository}/com/google/guava/guava/21.0/guava-21.0.jar"/>
      <zipfileset src="${path.variable.maven_repository}/org/apache/logging/log4j/log4j-core/2.0-beta9/log4j-core-2.0-beta9.jar"/>
    </jar>
    <copy file="${temp.jar.path.PickupSpawners.jar}" tofile="${artifact.temp.output.pickupspawners:jar}/PickupSpawners.jar"/>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.pickupspawners:jar" description="Build all artifacts">
    <mkdir dir="${artifact.output.pickupspawners:jar}"/>
    <copy todir="${artifact.output.pickupspawners:jar}">
      <fileset dir="${artifact.temp.output.pickupspawners:jar}"/>
    </copy>
    
    <!-- Delete temporary files -->
    <delete dir="${artifacts.temp.dir}"/>
  </target>
  
  <target name="all" depends="build.modules, build.all.artifacts" description="build all"/>
